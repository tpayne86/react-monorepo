{
  "name": "react-production-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "app:bootstrap": "lerna bootstrap",
    "app:clean": "lerna clean",
    "app:list": "lerna ls --all",
    "app:changes": "lerna changed",
    "app:diff": "lerna diff",
    "app:build": "yarn build:server && yarn build:commons && yarn wp:build --env.app=*",
    "app:start-server": "yarn execute start --scope=@healthifyme/server",
    "app:start-commons": "yarn execute:parallel  watch --ignore @healthifyme/server",
    "node:add-module": "lerna add --scope=",
    "node:outdated": "yarn outdated",
    "node:update-packages": "yarn upgrade",
    "git:push": "lerna version",
    "build": "yarn execute build",
    "build:package": "yarn build --scope",
    "build:all-packages": "yarn execute build",
    "build:server": "yarn build:package @healthifyme/server",
    "build:commons": "yarn build:all-packages --ignore @healthifyme/server",
    "test": "jest",
    "test:watch": "yarn test --watchAll",
    "exec": "lerna exec",
    "execute": "lerna run",
    "execute:parallel": "yarn execute  --parallel",
    "wp:start": "yarn wp:dev",
    "wp:dev": "yarn dev-server --mode=development --env.NODE_ENV=development --env.PROXY_ORIGIN=development",
    "wp:stage": "yarn dev-server --mode=development --env.NODE_ENV=development --env.PROXY_ORIGIN=staging",
    "wp:analyze-speed": "yarn wp:dev --env.addons=speedmeasure",
    "wp:build": "webpack --optimize-minimize --mode=production --env.NODE_ENV=production",
    "wp:build:analyze-bundle": "yarn wp:build  --env.addons=bundleanalyzer",
    "wp:build:analyze-buddy": "yarn wp:build --env.addons=bundlebuddy",
    "dev-server": "webpack-dev-server --inline --open",
    "wp:profile": "webpack --env.NODE_ENV=production --progress -c --profile --json  > stats.json",
    "project:init": "yarn config set workspaces-experimental true"
  },
  "proxy": {
    "development": "http://localhost:3000",
    "staging": "http://x123healthifyme.com"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "antd": "^3.9.0",
    "axios": "^0.18.0",
    "connected-react-router": "^4.4.1",
    "global": "^4.3.2",
    "history": "^4.7.2",
    "lerna": "^3.2.1",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-asynchronous-component": "^1.0.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.6",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "regenerator-runtime": "^0.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "antd-scss-theme-plugin": "^1.0.7",
    "autoprefixer": "^9.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "error-overlay-webpack-plugin": "^0.1.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-css-modules": "^2.8.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "extract-text-webpack-plugin": "*",
    "fast-sass-loader": "^1.4.6",
    "file-loader": "^2.0.0",
    "hard-source-webpack-plugin": "^0.12.0",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "speed-measure-webpack-plugin": "^1.2.2",
    "style-loader": "^0.23.0",
    "system-bell-webpack-plugin": "*",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "webpack": "4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.8",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.4"
  }
}
