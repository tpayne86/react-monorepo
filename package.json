{
  "name": "react-production-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "project:init": "yarn config set workspaces-experimental true",
    "app:bootstrap": "lerna bootstrap",
    "app:clean": "lerna clean",
    "app:list": "lerna ls --all",
    "app:changes": "lerna changed",
    "app:diff": "lerna diff",
    "app:build": "yarn build:commons && yarn build:server &&  yarn wp:build --env.app=*",
    "app:test": "yarn build:commons && yarn test",
    "app:start-server": "yarn execute start --scope=@healthifyme/server",
    "app:create-package": "yarn execute start --scope=@healthifyme/package-generator",
    "app:start-commons": "yarn execute:parallel  watch --ignore @healthifyme/server",
    "node:add-module": "lerna add --scope=",
    "node:outdated": "yarn outdated",
    "node:update": "yarn upgrade",
    "git:push": "lerna version",
    "build:package": "yarn build --scope",
    "build:all-packages": "yarn execute build",
    "build:server": "yarn build:package @healthifyme/server",
    "build:commons": "yarn build:all-packages --ignore @healthifyme/server",
    "build": "yarn execute build",
    "test": "jest",
    "test:watch": "yarn test --watchAll",
    "exec": "lerna exec",
    "execute": "lerna run",
    "execute:parallel": "yarn execute  --parallel",
    "wp:start": "yarn wp:dev",
    "wp:dev": "yarn dev-server --mode=development",
    "wp:analyze-speed": "yarn wp:dev --env.addons=speedmeasure",
    "wp:build": "webpack --optimize-minimize --mode=production --config ./packages/webpack-build/src/",
    "wp:build:analyze-bundle": "yarn wp:build  --env.addons=bundleanalyzer",
    "wp:build:analyze-buddy": "yarn wp:build --env.addons=bundlebuddy",
    "dev-server": "webpack-dev-server --config ./packages/webpack-build/src/",
    "wp:profile": "webpack --progress -c --profile --json  > stats.json",
    "lint:fix": "eslint  \"packages/**/*.{js,jsx}\" --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "antd": "^3.13.5",
    "axios": "^0.18.0",
    "connected-react-router": "^6.3.1",
    "history": "^4.7.2",
    "lerna": "^3.13.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-asynchronous-component": "^1.0.2",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.7.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^1.0.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "dotenv": "^6.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "error-overlay-webpack-plugin": "^0.1.6",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-watch": "^4.0.2",
    "extract-text-webpack-plugin": "*",
    "fast-sass-loader": "^1.4.7",
    "file-loader": "^3.0.1",
    "hard-source-webpack-plugin": "^0.13.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "speed-measure-webpack-plugin": "^1.3.0",
    "style-loader": "^0.23.0",
    "system-bell-webpack-plugin": "*",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-bundle-tracker": "^0.4.2-beta",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-node-externals": "^1.7.2",
    "webpack-s3-plugin": "^1.0.3"
  }
}
