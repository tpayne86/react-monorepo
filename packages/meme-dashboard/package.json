{
  "name": "@healthifyme/meme-dashboard",
  "version": "1.1.7",
  "description": "meme dashboard implements yarn workspaces and lerna project",
  "main": "index.js",
  "author": "hannad rehman",
  "license": "MIT",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "yarn dev-server --env.ENV_PORT=9000  --mode=development --env.NODE_ENV=development --env.PROXY_ORIGIN=development",
    "start:stage": "yarn dev-server --env.ENV_PORT=9000  --mode=development --env.NODE_ENV=development --env.PROXY_ORIGIN=staging",
    "start:analyze-speed": "yarn start:dev --env.addons=speedmeasure",
    "build": "yarn test && webpack --optimize-minimize --mode=production --env.NODE_ENV=production",
    "build:analyze-bundle": "yarn build  --env.addons=bundleanalyzer",
    "build:analyze-buddy": "yarn build --env.addons=bundlebuddy",
    "dev-server": "webpack-dev-server --inline",
    "test:watch": "yarn test --watchAll",
    "test": "export BABEL_ENV=test&&NODE_ENV=test&& jest",
    "profile": "webpack --env.NODE_ENV=production --progress -c --profile --json  > stats.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@healthifyme/components": "^1.1.3",
    "@healthifyme/elements": "^1.1.3",
    "@healthifyme/styles": "^1.0.2",
    "antd": "^3.9.0",
    "axios": "^0.18.0",
    "connected-react-router": "^4.4.1",
    "history": "^4.7.2",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-asynchronous-component": "^1.0.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.6",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "regenerator-runtime": "^0.12.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "eslint": "^5.5.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dev-server": {
    "port": 9001,
    "proxy": {
      "development": "http://localhost:3000",
      "staging": "http://x123healthifyme.com"
    }
  }
}
