{
  "name": "@healthifyme/constants",
  "version": "1.1.0",
  "description": "core constants for through out the application",
  "main": "dist/index.js",
  "scripts": {
    "watch": "yarn clean && yarn transpile --source-maps inline --watch",
    "build":"yarn clean && yarn env:prod && yarn transpile",
    "clean":"rm -rf ./dist",
    "transpile":"babel src --out-dir dist --copy-files --ignore spec.js,spec.jsx",
    "env:prod": "export NODE_ENV=production"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "eslint": "^5.5.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0"
  },
  "keywords": [
    "javascript",
    "constants",
    "healthifyme"
  ],
  "author": "hannad <hannad63@yahoo.com>",
  "license": "MIT"
}
