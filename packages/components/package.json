{
  "private": true,
  "name": "@healthifyme/components",
  "version": "2.0.0",
  "description": "a repository for react common components. may or may not be dependent on elements",
  "main": "dist/index.js",
  "author": "hannad rehman",
  "license": "MIT",
  "scripts": {
    "watch": "yarn clean && yarn transpile --source-maps inline --watch",
    "build": "yarn clean && yarn env:prod && yarn transpile",
    "clean": "rm -rf ./dist",
    "transpile": "babel src --out-dir dist --copy-files --ignore spec.js,spec.jsx",
    "env:prod": "export NODE_ENV=production"
  },
  "dependencies": {
    "@healthifyme/constants": "^2.0.0",
    "@healthifyme/elements": "^2.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "eslint": "^5.5.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0"
  }
}
