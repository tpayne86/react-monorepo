{
  "private": true,
  "name": "@healthifyme/elements",
  "version": "1.1.3",
  "description": "wrapper over all antd components. all library components should have a wrapper defined here",
  "main": "dist/index.js",
  "scripts": {
    "watch": "yarn clean && yarn transpile --source-maps inline --watch",
    "build": "yarn clean && yarn env:prod && yarn transpile",
    "clean": "rm -rf ./dist",
    "transpile": "babel src --out-dir dist --copy-files --ignore spec.js,spec.jsx",
    "env:prod": "export NODE_ENV=production"
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "keywords": [
    "antd",
    "react",
    "jest"
  ],
  "author": "hannad <hannad63@yahoo.com>",
  "license": "MIT",
  "dependencies": {
    "@healthifyme/constants": "^1.1.2",
    "react-asynchronous-component": "^1.0.2"
  },
  "peerDependencies": {
    "antd": "^3.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "eslint": "^5.5.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0"
  }
}
