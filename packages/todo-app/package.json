{
  "name": "@healthifyme/todo-app",
  "version": "1.2.0",
  "description": "meme dashboard implements yarn workspaces and lerna project",
  "main": "index.js",
  "author": "hannad rehman",
  "license": "MIT",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "yarn dev-server   --mode=development --env.NODE_ENV=development --env.PROXY_ORIGIN=http://localhost:3000",
    "start:stage": "yarn dev-server   --mode=development --env.NODE_ENV=development --env.PROXY_ORIGIN=https://x123healthifyme.com",
    "start:dev-min": "yarn start:dev  --env.MIN_STATS=true",
    "start:analyze-speed": "yarn start:dev --env.addons=speedmeasure",
    "build": "yarn test && webpack --optimize-minimize --mode=production --env.NODE_ENV=production",
    "build:analyze-bundle": "yarn build --env.addons=bundleanalyzer",
    "build:analyze-buddy": "yarn build --env.addons=bundlebuddy",
    "dev-server": "webpack-dev-server --inline",
    "test:watch": "yarn test --watchAll",
    "test": "export BABEL_ENV=test&&NODE_ENV=test&& jest",
    "profile": "webpack --env.NODE_ENV=production --progress -c --profile --json  > stats.json"
  },
  "dependencies": {
    "@healthifyme/components": "^1.1.3",
    "@healthifyme/elements": "^1.1.3",
    "@healthifyme/styles": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  }
}
