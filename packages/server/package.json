{
  "private": true,
  "name": "@nikaah/server",
  "version": "4.0.2",
  "description": "web server for healthifyme front end applications",
  "main": "index.js",
  "repository": "none",
  "author": "hannad rehman",
  "license": "MIT",
  "scripts": {
    "start": "yarn start:lint",
    "start:cluster": "yarn start:lint:cluster",
    "start:lint": "nodemon --exec 'babel-node ./src/index.js && yarn lint && node' ",
    "start:lint:cluster": "nodemon --exec 'babel-node' ./src/cluster.js && 'yarn lint && node' ",
    "start:no-lint": "nodemon --exec babel-node ./src/index.js",
    "build": "yarn clean && yarn transpile && node ./build",
    "lint": "esw ./src/",
    "clean": "rm -rf ../../dist",
    "transpile": "babel src --out-dir ../../dist --copy-files --ignore spec.js"
  },
  "devDependencies": {
    "eslint": "^5.11.0",
    "nodemon": "1.18.9"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "boom": "^7.3.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "http-proxy-middleware": "^0.19.0",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.0",
    "mysql2": "^1.6.4",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^3.3.0",
    "request": "^2.88.0",
    "sequelize": "^4.42.0"
  }
}
