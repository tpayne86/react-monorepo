{
  "private": true,
  "name": "@healthifyme/server",
  "version": "2.0.0",
  "description": "web server for healthifyme front end applications",
  "main": "index.js",
  "repository": "none",
  "author": "hannad rehman",
  "license": "MIT",
  "scripts": {
    "start": "export NODE_ENV=development&& export NODE_ENV_PORT=3000&& yarn start:lint",
    "start:cluster": "export NODE_ENV=development&& export NODE_ENV_PORT=3000&& yarn start:lint:cluster",
    "start:lint": "nodemon ./src/index.js --exec 'yarn lint && node' ",
    "start:lint:cluster": "nodemon ./src/cluster.js --exec 'yarn lint && node' ",
    "start:no-lint": "nodemon ./src/index.js",
    "build": "node build.js",
    "test:watch": "yarn test -- --watchAll",
    "test": "echo jest",
    "lint": "esw ./src/"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "nodemon": "*"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "http-proxy-middleware": "^0.19.0",
    "morgan": "^1.9.0",
    "request": "^2.88.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  }
}
