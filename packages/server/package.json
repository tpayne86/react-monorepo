{
  "private": true,
  "name": "@healthifyme/server",
  "version": "2.1.0",
  "description": "web server for healthifyme front end applications",
  "main": "index.js",
  "repository": "none",
  "author": "hannad rehman",
  "license": "MIT",
  "scripts": {
    "start": "yarn env:dev && yarn env:port && yarn start:lint",
    "start:cluster": "yarn env:dev&& yarn env:port&& yarn start:lint:cluster",
    "start:lint": "nodemon --exec 'babel-node ./src/index.js && yarn lint && node' ",
    "start:lint:cluster": "nodemon ./src/cluster.js --exec 'yarn lint && node' ",
    "start:no-lint": "nodemon --exec babel-node ./src/index.js",
    "build": "yarn clean && yarn env:prod && yarn transpile && node ./build",
    "lint": "esw ./src/",
    "env:port": "3000",
    "env:dev": "export NODE_ENV=development",
    "env:prod": "export NODE_ENV=production",
    "clean": "rm -rf ../../dist",
    "transpile": "babel src --out-dir ../../dist --copy-files --ignore spec.js"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "nodemon": "*"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "http-proxy-middleware": "^0.19.0",
    "morgan": "^1.9.0",
    "request": "^2.88.0"
  }
}
