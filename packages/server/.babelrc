{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current",
      },
    }],
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-transform-runtime",{"useESModules": true}],
    "@babel/plugin-transform-modules-commonjs"
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "current",
          },
        }],
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-proposal-object-rest-spread",
      ]
    },
    "development": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "current",
          },
        }],
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        ["@babel/plugin-transform-runtime",{"useESModules": true}],
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  }
}
