{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
"plugins": ["prettier"],
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "globals": {
    "module": true,
    "process":true
  },
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"],
    "import/namespace": 0,
    "react/jsx-one-expression-per-line":0
  }
}
